{
  "extends": "eslint:recommended",
  "env": {
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "error",
    "no-unused-vars": "error",
    "no-console": "warn",
    "no-nested-ternary": "error",
    "no-redeclare": "error",
    "no-param-reassign": "error"
  }
}
